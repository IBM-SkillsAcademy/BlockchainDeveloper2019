{
  "openapi": "3.0.0",
  "info": {
    "description": "Vehicle Manufacture Sample Application Smart Contract used for creating Blockchain Developer Course V2",
    "version": "1.0.0",
    "title": "Simple Application"
  },
  "paths": {
    "/api/v1/auth/enroll-admin": {
      "get": {
        "tags": [
          "Auth"
        ],
        "summary": "Enroll Admin",
        "responses": {
          "200": {
            "description": "Admin enrolled"
          }
        }
      }
    },
    "/api/v1/auth/register-user": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "Register user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "enrollmentID"
                ],
                "properties": {
                  "enrollmentID": {
                    "type": "string",
                    "example": "user1"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User registered"
          }
        }
      }
    },
    "/api/v1/vehicle": {
      "post": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Create a new vehicle",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "vehicleID",
                  "manufacturer",
                  "model",
                  "color",
                  "owner"
                ],
                "properties": {
                  "carID": {
                    "type": "string",
                    "example": "vehicle13"
                  },
                  "manufacturer": {
                    "type": "string",
                    "example": "Honda"
                  },
                  "model": {
                    "type": "string",
                    "example": "Accord"
                  },
                  "color": {
                    "type": "string",
                    "example": "Black"
                  },
                  "owner": {
                    "type": "string",
                    "example": "Tom"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vehicle created"
          }
        }
      },
      "get": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Get all vehicles",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "responses": {
          "200": {
            "description": "All vehicles listed",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/vehicle/{id}": {
      "get": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Get vehicle by id",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }, {
          "in": "path",
          "name": "id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "vehicle4"
          }
        }],
        "responses": {
          "200": {
            "description": "Vehicle listed",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    }
  },

  "components": {
  }
}