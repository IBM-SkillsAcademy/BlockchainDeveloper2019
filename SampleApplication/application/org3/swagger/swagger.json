{
  "openapi": "3.0.0",
  "info": {
    "description": "Vehicle Manufacture Sample Application Smart Contract used for creating Blockchain Developer Course V2",
    "version": "1.0.0",
    "title": "Insurer"
  },
  "paths": {
    "/api/v1/auth/enroll-admin": {
      "get": {
        "tags": [
          "Auth"
        ],
        "summary": "Enroll Admin",
        "responses": {
          "200": {
            "description": "Admin enrolled"
          }
        }
      }
    },
    "/api/v1/auth/create-affiliation": {
      "get": {
        "tags": [
          "Auth"
        ],
        "summary": "Create Affiliation org3",
        "responses": {
          "200": {
            "description": "Successsfullt created affiliation"
          }
        }
      }
    },
    "/api/v1/auth/register-user": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "Register user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "enrollmentID"
                ],
                "properties": {
                  "enrollmentID": {
                    "type": "string",
                    "example": "user1"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User registered"
          }
        }
      }
    },
    "/api/v1/vehicle": {
      "get": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Get all vehicles or by id",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }, {
          "in": "query",
          "name": "id",
          "schema": {
            "type": "string",
            "example": "vehicle13:Accord"
          }
        }],
        "responses": {
          "200": {
            "description": "All vehicles listed",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/vehicle/owner/change": {
      "post": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Change vehicle ownership",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "vehicleID",
                  "owner"
                ],
                "properties": {
                  "vehicleID": {
                    "type": "string",
                    "example": "vehicle13:Accord"
                  },
                  "owner": {
                    "type": "string",
                    "example": "Dave"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vehicle ownership changed"
          }
        }
      }
    },
    "/api/v1/vehicle/policy": {
      "put": {
        "tags": [
          "Policy"
        ],
        "summary": "issue insurance policy",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "id"
                ],
                "properties": {
                  "id": {
                    "type": "string",
                    "example": "policy1"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Policy"
        ],
        "summary": "Get all policies or by id",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }, {
          "in": "query",
          "name": "id",
          "schema": {
            "type": "string",
            "example": "insurerId:holderId"
          }
        }],
        "responses": {
          "200": {
            "description": "All vehicles listed",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/vehicle/delete": {
      "delete": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Delete vehicle",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "vehicleID"
                ],
                "properties": {
                  "vehicleID": {
                    "type": "string",
                    "example": "vehicle13:Accord"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vehicle deleted"
          }
        }
      }
    }
  },

  "components": {
  }
}