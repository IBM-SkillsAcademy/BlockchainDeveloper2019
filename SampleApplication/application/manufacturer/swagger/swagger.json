    "/api/v1/vehicle/order": {
      "post": {
        "tags": [
          "Order"
        ],
        "summary": "Create a new vehicle order",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "orderID",
                  "manufacturer",
                  "model",
                  "color",
                  "owner"
                ],
                "properties": {
                  "orderID": {
                    "type": "string",
                    "example": "vehicle13"
                  },
                  "manufacturer": {
                    "type": "string",
                    "example": "Honda"
                  },
                  "model": {
                    "type": "string",
                    "example": "Accord"
                  },
                  "color": {
                    "type": "string",
                    "example": "Black"
                  },
                  "owner": {
                    "type": "string",
                    "example": "Tom"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vehicle order issued"
          }
        }
      },
      "put": {
        "tags": [
          "Order"
        ],
        "summary": "Update vehicle order status",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "orderID",
                  "status"
                ],
                "properties": {
                  "orderID": {
                    "type": "string",
                    "example": "vehicle13"
                  },
                  "status": {
                    "type": "string",
                    "enum": [
                      "PENDING" ,
                      "INPROGRESS",
                      "DELIVERED"]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vehicle order updated"
          }
        }
      },
      "get": {
        "tags": [
          "Order"
        ],
        "summary": "Get all vehicle orders or by id",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }, {
          "in": "query",
          "name": "id",
          "schema": {
            "type": "string",
            "example": "vehicle13"
          }
        }],
        "responses": {
          "200": {
            "description": "All vehicles listed",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
   
