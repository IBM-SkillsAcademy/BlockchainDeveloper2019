    "/api/v1/vehicle/policy": {
      "post": {
        "tags": [
          "Policy"
        ],
        "summary": "request insurance policy",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "id",
                  "vehicleNumber",
                  "insurerId",
                  "holderId",
                  "policyType",
                  "startDate",
                  "endDate"
                ],
                "properties": {
                  "id": {
                    "type": "string",
                    "example": "policy1"
                  },
                  "vehicleNumber": {
                    "type": "string",
                    "example": "vehicle13:Accord"
                  },
                  "insurerId": {
                    "type": "string",
                    "example": "insurer1"
                  },
                  "holderId": {
                    "type": "string",
                    "example": "holder1"
                  },
                  "policyType": {
                    "type": "string",
                    "example": "THIRD_PARTY"
                  },
                  "startDate": {
                    "type": "string",
                    "example": "12122019"
                  },
                  "endDate": {
                    "type": "string",
                    "example": "31122019"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
