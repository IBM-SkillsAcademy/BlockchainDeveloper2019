     "/api/v1/vehicle/order/status/paginated": {
      "get": {
        "tags": [
          "Order"
        ],
        "summary": "Get all orders by status with pagination",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }, {
          "in": "query",
          "name": "orderStatus",
          "schema": {
            "type": "string",
            "enum": [
              "ISSUED",
              "PENDING" ,
              "INPROGRESS",
              "DELIVERED"]
          }
        } , 
         {
          "in": "query",
          "name": "pageSize",
          "schema": {
            "type": "string",
            "example" : "5"
          }
        } , 
         {
          "in": "query",
          "name": "bookmark",
          "schema": {
            "type": "string",
            "example" : ""
          }
        }],
        "responses": {
          "200": {
            "description": "All orders listed",
            "schema": {
