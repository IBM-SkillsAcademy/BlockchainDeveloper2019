{
  "openapi": "3.0.0",
  "info": {
    "description": "Vehicle Manufacture Sample Application Smart Contract used for creating Blockchain Developer Course V2",
    "version": "1.0.0",
    "title": "Regulator"
  },
  "paths": {
    "/api/v1/auth/enroll-admin": {
      "get": {
        "tags": [
          "Auth"
        ],
        "summary": "Enroll Admin",
        "responses": {
          "200": {
            "description": "Admin enrolled"
          }
        }
      }
    },
    "/api/v1/auth/register-user": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "Register user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "enrollmentID"
                ],
                "properties": {
                  "enrollmentID": {
                    "type": "string",
                    "example": "user1"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User registered"
          }
        }
      }
    },
    "/api/v1/vehicle": {
      "get": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Get all vehicles or by id",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }, {
          "in": "query",
          "name": "id",
          "schema": {
            "type": "string",
            "example": "vehicle13"
          }
        }],
        "responses": {
          "200": {
            "description": "All vehicles listed",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/vehicle/price": {
      "get": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Get vehicle price",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }, {
          "in": "query",
          "name": "id",
          "schema": {
            "type": "string",
            "example": "vehicle13"
          }
        }],
        "responses": {
          "200": {
            "description": "Vehicle price listed"
          }
        }
      }
    },
    "/api/v1/vehicle/change-owner": {
      "post": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Change vehicle ownership",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "vehicleID",
                  "owner"
                ],
                "properties": {
                  "vehicleID": {
                    "type": "string",
                    "example": "vehicle13"
                  },
                  "owner": {
                    "type": "string",
                    "example": "Dave"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vehicle ownership changed"
          }
        }
      }
    },
    "/api/v1/vehicle/issue-vin": {
      "post": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Issue vehicle VIN",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "vehicleID",
                  "vin"
                ],
                "properties": {
                  "vehicleID": {
                    "type": "string",
                    "example": "vehicle13"
                  },
                  "vin": {
                    "type": "string",
                    "example": "G33KS"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vehicle VIN issued"
          }
        }
      }
    },
    "/api/v1/vehicle/delete": {
      "delete": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Delete vehicle",
        "parameters": [{
          "in": "header",
          "name": "enrollment-id",
          "required": "true",
          "schema": {
            "type": "string",
            "example": "user1"
          }
        }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "vehicleID"
                ],
                "properties": {
                  "vehicleID": {
                    "type": "string",
                    "example": "vehicle13"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vehicle deleted"
          }
        }
      }
    }
  },

  "components": {
  }
}